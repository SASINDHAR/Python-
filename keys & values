def main():
    # Initialize a dictionary with keys and values
    my_dictionary = {
        "apple": 5,
        "banana": 10,
        "orange": 7,
        "grape": 3
    }

    # Print the contents of the dictionary
    print("Contents of the dictionary:")
    for key, value in my_dictionary.items():
        print(key, ":", value)

if __name__ == "__main__":
    main()
